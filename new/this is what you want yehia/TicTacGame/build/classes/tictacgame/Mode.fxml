<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictacgame.Mode">
    <children>
      <Button layoutX="230.0" layoutY="181.0" mnemonicParsing="false" onMouseClicked="#computer" prefHeight="38.0" prefWidth="139.0" text="Computer">
         <font>
            <Font name="Franklin Gothic Medium Italic" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect></Button>
      <MenuButton layoutX="230.0" layoutY="123.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="139.0" text="2 Players">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#onLine" text="Online" />
          <MenuItem mnemonicParsing="false" onAction="#offLine" text="Offline" />
        </items>
         <font>
            <Font name="Franklin Gothic Medium Italic" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect>
      </MenuButton>
      <Button layoutX="230.0" layoutY="233.0" mnemonicParsing="false" onMouseClicked="#back" prefHeight="38.0" prefWidth="139.0" text="Back">
         <font>
            <Font name="Franklin Gothic Medium Italic" size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <InnerShadow />
         </effect></Button>
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</AnchorPane>
